{
	"name": "logview",
	"displayName": "LogView",
	"description": "make log visual",
	"version": "0.0.3",
	"publisher": "freeman",
	"icon": "resource/logo.png",
	"repository": {
		"url": "https://github.com/YigaoFan/logview"
	},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:log",
		"onView:logAnalyze"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsWelcome": [
			{
				"view": "logAnalyze",
				"contents": "No valid log file opened"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "LogResult",
					"title": "Log Result",
					"icon": "resource/content.png"
				}
			]
		},
		"commands": [
			{
				"command": "logAnalyze.refresh",
				"title": "Refresh",
				"icon": "resource/refresh.svg"
			},
			{
				"command": "logAnalyze.goHere",
				"title": "go here",
				"icon": "resource/go.svg"
			}
		],
		"languages": [
			{
				"id": "log",
				"extensions": [
					".log"
				]
			}
		],
		"views": {
			"LogResult": [
				{
					"id": "logAnalyze",
					"name": "Log Analyze",
					"contextualTitle": "Analyze result"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "logAnalyze.refresh",
					"when": "view == logAnalyze",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "logAnalyze.goHere",
					"when": "view == logAnalyze",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}
